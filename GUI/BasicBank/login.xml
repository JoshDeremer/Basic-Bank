<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE gridbag SYSTEM "gridbag.dtd">

<!-- XML Document used to create my Login to Account form. -->

<gridbag>
	<row>
		<cell anchor="WEST" gridx="0" gridy="0">
			<bean>
				<class>javax.swing.JLabel</class>
				<property>
					<name>text</name>
					<value><string>Name: </string></value>
				</property>
			</bean>
		</cell>
		<cell gridx="1" gridy="0" fill="HORIZONTAL">
			<bean id="login">
				<class>javax.swing.JTextArea</class>
				<property>
		          <name>border</name>
		          <value><bean>
		            <class>javax.swing.border.EtchedBorder</class>
		          </bean></value>
		        </property>
		    </bean>
		</cell>
	</row>
	<row>
		<cell anchor="WEST" gridx="0" gridy="1">
			<bean>
				<class>javax.swing.JLabel</class>
				<property>
					<name>text</name>
					<value><string>Password: </string></value>
				</property>
			</bean>
		</cell>
		<cell gridx="1" gridy="1" fill="HORIZONTAL">
			<bean id="password">
				<class>javax.swing.JPasswordField</class>
				<property>
		          <name>border</name>
		          <value><bean>
		            <class>javax.swing.border.EtchedBorder</class>
		          </bean></value>
		        </property>
		    </bean>
		</cell>
	</row>
   
   
   
	<row>
		<cell gridx="0" gridy="2" gridwidth="2">
			<bean id="captcha">
				<class>javax.swing.JLabel</class>
				<property>
					<name>text</name>
					<value><string>A</string></value>
				</property>
			</bean>
		</cell>
   </row>
   <row>
		<cell gridx="0" gridy="3" gridwidth="2">
			<bean id="captcha2">
				<class>javax.swing.JLabel</class>
				<property>
					<name>text</name>
					<value><string>B</string></value>
				</property>
			</bean>
		</cell>
   </row>
   <row>
		<cell gridx="0" gridy="4" gridwidth="2" fill="HORIZONTAL">
			<bean id="captcha_try">
				<class>javax.swing.JTextArea</class>
				<property>
		          <name>border</name>
		          <value><bean>
		            <class>javax.swing.border.EtchedBorder</class>
		          </bean></value>
		        </property>
		    </bean>
		</cell>
	</row>
   
   
   
	<row>
		<cell anchor="WEST" gridx="0" gridy="5" ipadx="25">
			<bean id="login_button">
				<class>javax.swing.JButton</class>
				<property>
					<name>text</name>
					<value><string>Login</string></value>
				</property>
			</bean>
		</cell>
		<cell anchor="EAST" gridx="1" gridy="5" ipadx="25">
			<bean id="cancel_button">
				<class>javax.swing.JButton</class>
				<property>
					<name>text</name>
					<value><string>Cancel</string></value>
				</property>
			</bean>
		</cell>
	</row>
   
   
   
	<row>
		<cell gridx="0" gridy="6" gridwidth="2">
			<bean id="failure">
				<class>javax.swing.JLabel</class>
				<property>
					<name>text</name>
					<value><string>C</string></value>
				</property>
			</bean>
		</cell>
	</row>
   
   
   
</gridbag>
